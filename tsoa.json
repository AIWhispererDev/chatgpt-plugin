{
  "entryFile": "src/index.ts",
  "noImplicitAdditionalProperties": "throw-on-extras",
  "controllerPathGlobs": [
    "src/plugin/*Controller.ts"
  ],
  "ignore": [
    "src/generated"
  ],
  "spec": {
    "outputDirectory": "./",
    "specVersion": 3,
    "host": "localhost:3000",
    "yaml": true,
    "specFileBaseName": "openapi"
  },
  "routes": {
    "routesDir": "src/generated"
  }
}